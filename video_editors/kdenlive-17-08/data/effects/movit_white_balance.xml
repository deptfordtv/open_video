<!DOCTYPE kpartgui>
<effect tag="movit.white_balance" id="movit.white_balance">
    <name>White Balance (GPU)</name>
    <author>Steinar H. Gunderson</author>
    <parameter type="color" name="neutral_color" default="0x7f7f7fff">
	<name>Neutral Color</name>
